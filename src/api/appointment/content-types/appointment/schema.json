{
  "kind": "collectionType",
  "collectionName": "appointments",
  "info": {
    "singularName": "appointment",
    "pluralName": "appointments",
    "displayName": "Appointment"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "ClientName": {
      "type": "string",
      "required": true
    },
    "ClientEmail": {
      "type": "email"
    },
    "ClientPhone": {
      "type": "string"
    },
    "AppointmentDateTime": {
      "type": "datetime"
    },
    "BookingStatus": {
      "type": "enumeration",
      "enum": [
        "Pending",
        "Confirmed",
        "Completed",
        "Cancelled",
        "No-Show"
      ]
    },
    "booked_services": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::service.service",
      "inversedBy": "booked_appointments"
    },
    "SelectedStaff": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::staff.staff"
    },
    "TotalAmount": {
      "type": "decimal"
    }
  }
}
