{
  "kind": "collectionType",
  "collectionName": "services",
  "info": {
    "singularName": "service",
    "pluralName": "services",
    "displayName": "Service"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "Name": {
      "type": "string",
      "required": true
    },
    "Description": {
      "type": "blocks",
      "required": false
    },
    "Price": {
      "type": "decimal",
      "required": true
    },
    "Duration": {
      "type": "integer",
      "required": true
    },
    "Deposit": {
      "type": "decimal",
      "default": 10000
    },
    "Picture": {
      "type": "media",
      "multiple": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "OnSalesPrice": {
      "type": "decimal"
    },
    "OnSaleTitle": {
      "type": "string"
    },
    "IsAddOn": {
      "type": "boolean",
      "default": false
    },
    "booked_appointments": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::appointment.appointment",
      "mappedBy": "booked_services"
    }
  }
}
